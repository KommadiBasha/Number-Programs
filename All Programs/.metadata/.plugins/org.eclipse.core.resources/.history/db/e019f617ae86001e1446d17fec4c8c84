package Exam;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) 
	{
		Scanner sc= new Scanner (System.in);
		System.out.println("enter a number");
		int num=sc.nextInt();
		int pro=1;
		for(int i=1;i<=num;i++)
		{
			pro=pro*i;
			int store=num+pro;
			System.out.println(store);
		}
		
	}

}
