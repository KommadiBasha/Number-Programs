package Exam;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) 
	{
		Scanner sc= new Scanner (System.in);
		System.out.println("enter a number");
		int num1=sc.nextInt();
		int num2=sc.nextInt();
		int temp=1;
		while(num1>=0)
		{
			int fact=num1*num2;
			System.out.println(fact);
		}
	}

}
