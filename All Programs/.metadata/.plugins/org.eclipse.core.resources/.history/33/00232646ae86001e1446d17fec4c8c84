package Exam;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) 
	{
		Scanner sc= new Scanner (System.in);
		System.out.println("enter a number");
		int num=sc.nextInt();
	
		for(int i=1;i<=num;i++)
		{
			int temp=i;
			int fact=1;
			for(int j=1;j<=temp;j++)
			{
				fact=fact*j;
			}
			System.out.println(fact);
		}
		
	}

}
