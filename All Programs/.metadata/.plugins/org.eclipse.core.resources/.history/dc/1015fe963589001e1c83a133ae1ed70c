package SecondTimeAllPrograms;

import java.util.Scanner;

public class PalindromeNumberorNot {

	public static void main(String[] args) 
	{
		Scanner sc= new Scanner(System.in);
		int num=sc.nextInt();
		int temp=num;
		int last=0;
		boolean b=false;
		while(num!=0)
		{
			int store=num%10;
		
		
			if(temp==store)
			{
				b=true;
			}
			num/=10;
		}

	}

}
